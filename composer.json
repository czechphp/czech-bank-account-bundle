{
    "name": "czechphp/czech-bank-account-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration for Czech bank account library",
    "keywords": [
        "bank account number",
        "bank code",
        "payment system code",
        "variable symbol",
        "specific symbol",
        "kód banky",
        "číselník kódů platebního styku",
        "Czech republic",
        "Czechia",
        "Symfony",
        "Bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.4 || ^4 || ^5",
        "symfony/dependency-injection": "^3.4 || ^4 || ^5",
        "symfony/form": "^3.4 || ^4 || ^5",
        "symfony/http-kernel": "^3.4 || ^4 || ^5",
        "symfony/validator": "^3.4 || ^4 || ^5",
        "czechphp/czech-bank-account": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "authors": [
        {
            "name": "Lukáš Mencl",
            "email": "tetra.jinak@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Czechphp\\CzechBankAccountBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Czechphp\\CzechBankAccountBundle\\Tests\\": "Tests/"
        }
    }
}
